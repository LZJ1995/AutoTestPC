<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.gac.lzj.avnt.entities.BorrowMaterielEntity" table="borrow_materiel" schema="avntmanagement">
        <id name="borrowMaterielId">
            <column name="borrow_materiel_Id" sql-type="int(11)"/>
        </id>
        <property name="borrowTime">
            <column name="borrow_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="borrowReturnTime">
            <column name="borrow_ReturnTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="borrowPurpose">
            <column name="borrow_Purpose" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="borrowCount">
            <column name="borrow_count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="borrowStates">
            <column name="borrow_States" sql-type="varchar(1)" length="1" not-null="true"/>
        </property>
        <property name="borrowName">
            <column name="borrow_Name" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="borrowPhone">
            <column name="borrow_Phone" sql-type="int(20)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>